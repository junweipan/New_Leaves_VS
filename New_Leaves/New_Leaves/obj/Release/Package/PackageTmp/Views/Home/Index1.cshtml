@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!--Main Slider-->
<section class="main-slider" data-start-height="750" data-slide-overlay="yes">

    <div class="tp-banner-container">
        <div class="tp-banner">
            <ul>

                <li data-transition="fade" data-slotamount="1" data-masterspeed="1000" data-thumb="images/slide-image-1.jpg" data-saveperformance="off" data-title="Awesome Title Here">
                    <img src="images/slide-image-1.jpg" alt="" data-bgposition="center top" data-bgfit="cover" data-bgrepeat="no-repeat">

                    <div class="tp-caption sfb sfb tp-resizeme"
                         data-x="center" data-hoffset="0"
                         data-y="center" data-voffset="0"
                         data-speed="1500"
                         data-start="500"
                         data-easing="easeOutExpo"
                         data-splitin="none"
                         data-splitout="none"
                         data-elementdelay="0.01"
                         data-endelementdelay="0.3"
                         data-endspeed="1200"
                         data-endeasing="Power4.easeIn">
                        <div class="slide-content">
                            <div class="slider-border"></div>
                            <div class="title">Save the Children</div>
                            <h2>Make Them Smile Again </h2>
                            <div class="text">All of our wish is to keep them smiling</div>
                            <a href="#" class="theme-btn btn-style-two">Learn more</a>
                        </div>
                    </div>

                </li>

                <li data-transition="fade" data-slotamount="1" data-masterspeed="1000" data-thumb="images/slide-image-2.jpg" data-saveperformance="off" data-title="Awesome Title Here">
