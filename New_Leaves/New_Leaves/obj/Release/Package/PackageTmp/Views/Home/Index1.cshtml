                         data-speed="1500"
                         data-start="500"
                         data-easing="easeOutExpo"
                         data-splitin="none"
                         data-splitout="none"
                         data-elementdelay="0.01"
                         data-endelementdelay="0.3"
                         data-endspeed="1200"
                         data-endeasing="Power4.easeIn">
                        <div class="slide-content">
                            <div class="slider-border"></div>
                            <div class="title">better future</div>
                            <h2>Welcome to New Home</h2>
                            <div class="text">New Leaves offer New Lives</div>
                            <a href="#" class="theme-btn btn-style-two">Learn more</a>
                        </div>
                    </div>

                </li>

                <li data-transition="fade" data-slotamount="1" data-masterspeed="1000" data-thumb="images/slide-image-3.jpg" data-saveperformance="off" data-title="Awesome Title Here">
