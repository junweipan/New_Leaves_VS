@model IEnumerable<New_Leaves.Models.Donation>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Donation History</h2>
<p>
    @Html.ActionLink("Return to my account", "DonorDetails","DonorManage",new { code = User.Identity.Name},null)
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Item_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Donate_Date)
        </th>
      
        
        <th>
            @Html.DisplayNameFor(model => model.Wish_List.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Item_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Donate_Date)
        </td>
      
       
        <td>
            @Html.DisplayFor(modelItem => item.Wish_List.Status)
        </td>
    </tr>
}

</table>
